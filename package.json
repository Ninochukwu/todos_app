{
  "name": "api-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
